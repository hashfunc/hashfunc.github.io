{
  "presets": [
    "next/babel"
  ],
  "plugins": [
    [
      "styled-components",
      {
        "ssr": true
      }
    ],
    [
      "babel-plugin-module-resolver",
      {
        "alias": {
          "assets": "./assets",
          "components": "./components",
          "styles": "./styles"
        }
      }
    ]
  ]
}
